{
    "pipeline": {
        "name": "devops-pipeline-pipeline",
        "roleArn": "arn:aws:iam::070593201734:role/devops-pipeline-codepipeline-role",
        "artifactStore": {
            "type": "S3",
            "location": "devops-pipeline-artifacts-devops200406"
        },
        "stages": [
            {
                "name": "Source",
                "actions": [
                    {
                        "name": "Source",
                        "actionTypeId": {
                            "category": "Source",
                            "owner": "AWS",
                            "provider": "S3",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "S3Bucket": "devops-pipeline-artifacts-devops200406",
                            "S3ObjectKey": "source.zip"
                        },
                        "outputArtifacts": [
                            {
                                "name": "source_output"
                            }
                        ],
                        "inputArtifacts": []
                    }
                ]
            },
            {
                "name": "Build",
                "actions": [
                    {
                        "name": "Build",
                        "actionTypeId": {
                            "category": "Build",
                            "owner": "AWS",
                            "provider": "CodeBuild",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "ProjectName": "devops-pipeline-build"
                        },
                        "outputArtifacts": [
                            {
                                "name": "build_output"
                            }
                        ],
                        "inputArtifacts": [
                            {
                                "name": "source_output"
                            }
                        ]
                    }
                ]
            },
            {
                "name": "Deploy",
                "actions": [
                    {
                        "name": "Deploy",
                        "actionTypeId": {
                            "category": "Deploy",
                            "owner": "AWS",
                            "provider": "ECS",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "ClusterName": "devops-pipeline-cluster",
                            "ServiceName": "devops-pipeline-service",
                            "FileName": "imagedefinitions.json"
                        },
                        "inputArtifacts": [
                            {
                                "name": "build_output"
                            }
                        ],
                        "outputArtifacts": []
                    }
                ]
            }
        ],
        "version": 5
    }
}
