# Example Terraform variables file
# Copy this file to terraform.tfvars and update with your values

# AWS Region where resources will be created
aws_region = "us-east-1"

# Name of your project (will be used as prefix for resources)
project_name = "devops-pipeline"

# S3 bucket name for storing pipeline artifacts (must be globally unique)
artifact_bucket_name = "devops-pipeline-artifacts-YOUR-UNIQUE-ID"

# Your GitHub repository in format owner/repo
github_repo = "YOUR-USERNAME/devops-pipeline"

# GitHub branch to monitor for changes
github_branch = "main"

# GitHub personal access token (will be prompted if not set)
# github_token = "your-github-token-here"
